# zsh custom aliases
# $ZSH_CUSTOM/aliases.zsh
alias la='ls -A'

alias dir='dir --color=auto'
alias vdir='vdir --color=auto'

alias grep='grep --color=auto'
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'

alias nano='nano -mc'

# git aliases
alias g='git'
alias gc='git checkout'
alias gb='git branch'
alias gs='git status'

setopt cdablevars
ghub="/c/GitHub/"
c="/c/"
